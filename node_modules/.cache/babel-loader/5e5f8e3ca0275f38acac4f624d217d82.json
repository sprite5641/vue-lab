{"ast":null,"code":"import IndexNavbar from \"@/components/Navbars/IndexNavbar.vue\";\nimport FooterComponent from \"@/components/Footers/Footer.vue\";\nimport patternVue from \"@/assets/img/pattern_vue.png\";\nimport componentBtn from \"@/assets/img/component-btn.png\";\nimport componentProfileCard from \"@/assets/img/component-profile-card.png\";\nimport componentInfoCard from \"@/assets/img/component-info-card.png\";\nimport componentInfo2 from \"@/assets/img/component-info-2.png\";\nimport componentMenu from \"@/assets/img/component-menu.png\";\nimport componentBtnPink from \"@/assets/img/component-btn-pink.png\";\nimport documentation from \"@/assets/img/documentation.png\";\nimport login from \"@/assets/img/login.jpg\";\nimport profile from \"@/assets/img/profile.jpg\";\nimport landing from \"@/assets/img/landing.jpg\";\nexport default {\n  data: function data() {\n    return {\n      patternVue: patternVue,\n      componentBtn: componentBtn,\n      componentProfileCard: componentProfileCard,\n      componentInfoCard: componentInfoCard,\n      componentInfo2: componentInfo2,\n      componentMenu: componentMenu,\n      componentBtnPink: componentBtnPink,\n      documentation: documentation,\n      login: login,\n      profile: profile,\n      landing: landing\n    };\n  },\n  components: {\n    IndexNavbar: IndexNavbar,\n    FooterComponent: FooterComponent\n  }\n};","map":{"version":3,"sources":["C:\\Users\\Sprite-PC\\OneDrive\\เดสก์ท็อป\\vue-notus\\src\\pages\\Index.vue"],"names":[],"mappings":"AA8uBA,OAAO,WAAP,MAAwB,sCAAxB;AACA,OAAO,eAAP,MAA4B,iCAA5B;AAEA,OAAO,UAAP,MAAuB,8BAAvB;AACA,OAAO,YAAP,MAAyB,gCAAzB;AACA,OAAO,oBAAP,MAAiC,yCAAjC;AACA,OAAO,iBAAP,MAA8B,sCAA9B;AACA,OAAO,cAAP,MAA2B,mCAA3B;AACA,OAAO,aAAP,MAA0B,iCAA1B;AACA,OAAO,gBAAP,MAA6B,qCAA7B;AACA,OAAO,aAAP,MAA0B,gCAA1B;AACA,OAAO,KAAP,MAAkB,wBAAlB;AACA,OAAO,OAAP,MAAoB,0BAApB;AACA,OAAO,OAAP,MAAoB,0BAApB;AAEA,eAAe;AACb,EAAA,IADa,kBACN;AACL,WAAO;AACL,MAAA,UAAU,EAAV,UADK;AAEL,MAAA,YAAY,EAAZ,YAFK;AAGL,MAAA,oBA